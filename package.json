{
  "name": "kzou-server",
  "version": "0.3.0",
  "description": "Server API for Kzou App",
  "engines": {
    "node": "8.9.1"
  },
  "main": "index.js",
  "author": "Alexgalinier",
  "scripts": {
    "pretty": "a_codestyle -i build",
    "build-js": "babel src -d build",
    "watch-js": "babel src -d build -w",
    "watch-server": "nodemon --watch build build/index.js",
    "start": "run-p watch-js watch-server",
    "start-local-prod": "NODE_ENV=production HTTP_HEADER_ORIGIN=* node index.js",
    "test": "jest"
  },
  "dependencies": {
    "dotenv": "^5.0.0",
    "jsonwebtoken": "^8.2.0",
    "lodash": "^4.17.4",
    "mongodb": "^3.0.2",
    "shortid": "^2.2.8"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.44",
    "@babel/core": "^7.0.0-beta.44",
    "@babel/preset-env": "^7.0.0-beta.44",
    "a_codestyle": "^3.0.1",
    "jest": "^21.2.1",
    "nodemon": "^1.17.3",
    "npm-run-all": "^4.1.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Alexgalinier/kzou-server"
  },
  "keywords": [
    "education",
    "freinet",
    "backend"
  ],
  "license": "MIT"
}
